{
  "name": "sistema-gerenciamento-gruas-api",
  "version": "1.0.0",
  "description": "API REST para Sistema de Gerenciamento de Gruas",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:clientes": "jest src/routes/tests/clientes.test.cjs",
    "test:clientes:integration": "node src/routes/tests/clientes-integration.test.js",
    "test:clientes:real": "node src/routes/tests/clientes-integration-real.test.js",
    "test:clientes:database": "node src/routes/tests/clientes-database.test.js",
    "test:clientes:db-corrected": "node src/routes/tests/clientes-database-corrected.test.js",
    "docs": "swagger-jsdoc -d swaggerDef.js src/routes/*.js -o docs/swagger.json"
  },
  "keywords": [
    "api",
    "rest",
    "gruas",
    "gerenciamento",
    "express"
  ],
  "author": "Sistema IRBANA",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "@supabase/supabase-js": "^2.38.4",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
